{
  "name": "Demo",
  "version": "",
  "scripts": {
    "dev": "webpack-dev-server -d --inline --hot --open --env.dev",
    "mock": "webpack-dev-server -d --inline --hot --env.dev --env.mock",
    "build": "rimraf dist && npm run lint && webpack -p --progress",
    "build-dev": "rimraf dist && npm run lint && webpack -d --progress",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.js src/**/*.vue",
    "lint-fix": "node node_modules/eslint/bin/eslint.js src/**/*.js src/**/*.vue --fix"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "echarts": "^3.7.2",
    "element-ui": "^2.0.2",
    "element-ui-plugin": "0.0.2",
    "querystring": "^0.1.0",
    "vue": "^2.3.3",
    "vue-resource": "^1.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.13.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-enough": "^0.2.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^3.2.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "git-commit-hook": "^1.1.4",
    "glob": "^7.1.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "object-assign": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "2.2.0",
    "webpack-hot-middleware": "^2.17.1"
  },
  "pre-commit": {

    "pass": [
      "^merge",
      "^first\\s+commit",
      "^reversion",
      "^update\\s+[a-zA-Z0-9\\/\\-\\._]*.(json|md|gitignore|bowerrc|npmrc|cnpmrc)"
    ],
    "scripts": {
      "precommit": "npm run lint",
      "postcommit": ""
    }
  },
  "author": "hansh,huangl"
}
